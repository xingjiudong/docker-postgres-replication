# Docker volume driver
export VOLUME_DRIVER=rbd

# Etcd Endpoints IP
export ETCD_CLIENT_IP=

# For docker swarm cluster
export DOCKER_SWARM_URI=
export COMPOSE_OPT=

# Versions
export POSTGRES_VERSION=latest
export HAPROXY_VERSION=latest

# POSTGRES Information
export POSTGRES_USER=postgres
export POSTGRES_PASSWORD=postgres
export DNS_SERVER=

# Haproxy Information
export HAPROXY_NODE=
export PROJECT_NAME=pg
export TIMEOUT_CONNECT=30s
export TIMEOUT_CLIENT=5m
export TIMEOUT_SERVER=5m
export TIMEOUT_HTTP_KEEP_ALIVE=10s
export MAXCONN=2000

